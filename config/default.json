{
  "index-format": [
    {
      "index": "./src/index.ts",
      "module": {
        "includes": ["./src"],
        "excludes": ["./src/lib", "./src/StackHistory", "./**/*.test.ts"]
      }
    },
    {
      "index": "./src/lib/index.ts",
      "module": {
        "includes": ["./src/lib/"],
        "excludes": ["./**/*.test.ts"]
      }
    }
  ]
}
